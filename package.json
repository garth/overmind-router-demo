{
  "name": "overmind-router",
  "version": "1.0.0",
  "author": "Garth Williams <garth@wi.llia.ms>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "devtools": "overmind-devtools",
    "prebuild": "rimraf dist",
    "build": "parcel build src/index.html",
    "start": "parcel src/index.html --port 3000",
    "prettier": "prettier --write \"src/**/*.js\" \"src/**/*.ts\" \"src/**/*.tsx\" \"src/**/*.scss\""
  },
  "prettier": {
    "printWidth": 120,
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "jsxBracketSameLine": true
  },
  "devDependencies": {
    "@types/page": "^1.8.0",
    "@types/react": "^16.4.14",
    "@types/react-dom": "^16.0.7",
    "node-sass": "^4.9.3",
    "npm-run-all": "^4.1.3",
    "overmind-devtools": "^2.0.0",
    "parcel": "^1.9.7",
    "prettier": "^1.14.2",
    "rimraf": "^2.6.2",
    "typescript": "^3.0.3"
  },
  "dependencies": {
    "bulma": "^0.7.1",
    "overmind": "^2.0.0",
    "overmind-react": "^2.0.0",
    "page": "^1.10.1",
    "react": "^16.5.0",
    "react-dom": "^16.5.0"
  }
}
